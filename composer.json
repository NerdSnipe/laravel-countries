{
    "name": "nerdsnipe/laravel-countries",
    "description": "Used to add country, state, city selectors to any laravel view. Provides all cities and states for 251 countries",
    "keywords": [
        "NerdSnipe",
        "laravel",
        "laravel-countries",
        "laravel country data",
        "laravel state data",
        "laravel city data",
        "country state city dynamic dependent dropdown",
        "country state city dropdown"
    ],
    "homepage": "https://github.com/nerdsnipe/laravel-countries",
    "license": "MIT",
    "authors": [
        {
            "name": "NerdSnipe Inc",
            "email": "hello@nerdsnipe.cc",
            "homepage": "https://www.nerdsnipe.cc",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.19",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "NerdSnipe\\LaravelCountries\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NerdSnipe\\LaravelCountries\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NerdSnipe\\LaravelCountries\\LaravelCountriesServiceProvider"
            ],
            "aliases": {
                "LaravelCountries": "NerdSnipe\\LaravelCountries\\Facades\\LaravelCountries"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}